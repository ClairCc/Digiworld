<!-- Header -->
<%- include('layautsDashboard/header') %>

<body class="main-body leftmenu body-ingreso">

  <%- include('layautsDashboard/loader') %>

  <!-- Page -->
  <div class="page">

    <!-- Menu -->
    <%- include('layautsDashboard/menu') %>

    <!-- Cabecera -->
    <%- include('layautsDashboard/head') %>

    <!-- Main Content-->
    <div class="main-content side-content pt-0">

      <div class="container-fluid">
        <div class="inner-body">

          <!-- Page Header -->
          <div class="page-header">
            <div>
              <h2 class="main-content-title tx-24 mg-b-5"><%- titulo %></h2>
              <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="/dashboard/inicio">Inicio</a></li>
                <li class="breadcrumb-item active" aria-current="page"><%- breadcrumb %></li>
              </ol>
            </div>
            <div class="d-flex">
              <div class="justify-content-center">
                <a class="btn btn-primary my-2 btn-icon-text" href="/dashboard/reportarConsignacion">
                  <i class="ti-wallet mr-2"></i> Recarga tu cuenta
                </a>
              </div>
            </div>
          </div>
          <!-- End Page Header -->

          <!-- =============================
							Inicio Componentes Pagina
						============================== -->
          <div class="row row-sm">
            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6">
              <div class="card custom-card">
                <div class="card-body">
                  <div class="card-widget">
                    <% if(usuario.pais === 'Colombia') { %>
                    <% var saldoFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(usuario.saldo); %>
                    <% } else { %>
                    <% var saldoFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(usuario.saldo); %>
                    <% } %>
                    <label class="main-content-label mb-3 pt-1">Saldo Actual Disponible</label>
                    <h2 class="text-right"><i class="icon-size fas fa-coins float-left text-primary"></i><span class="font-weight-bold"><%- saldoFormateado %></span></h2>
                    <p class="mb-0 text-muted">Fecha Actual<span class="float-right"><%- fecha %></span></p>
                  </div>
                </div>
              </div>
            </div>
            <!-- COL END -->
            <div class="col-sm-12 col-md-6 col-lg-6 col-xl-6">
              <div class="card custom-card">
                <div class="card-body">
                  <div class="card-widget">
                    <label class="main-content-label mb-3 pt-1">Plataformas Disponibles</label>
                    <h2 class="text-right"><i class="icon-size fe fe-cast float-left text-primary"></i><span class="font-weight-bold"><%- plataformas.length %></span></h2>
                    <p class="mb-0 text-muted">Directas, Renovaciones; Personalizadas, Juegos, Bajo pedido</p>
                  </div>
                </div>
              </div>
            </div>
            <!-- COL END -->
          </div>

          <!-- ======================
                            Plataformas Normales
                        ======================= -->

          <h2 class="main-content-title tx-24 mg-b-5 mt-4">Cuentas</h2>
          <p class="text-muted">Usted podra comprar las siguientes cuentas de forma inmediata.</p>
          <div class="row row-sm">

            <% if(plataformasNormales.length) { %>
            <% plataformasNormales.forEach(listaPlataformasNormales => { %>

            <div class="col-sm-12 col-md-3">
              <div class="card custom-card our-team">
                <div class="card-body">
                  <div class="mb-3 circulo-etiqueta"></div>
                  <div class="picture avatar-lg online text-center">
                    <img alt="avatar" class="rounded-circle" src="<%- listaPlataformasNormales.plataforma.logo %>">
                  </div>
                  <div class="text-center mt-3">
                    <h5 class="pro-user-username text-dark mt-2 mb-0"><%- listaPlataformasNormales.plataforma.plataforma %></h5>
                    <p class="user-info-rating mt-2">
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                    </p>
                  </div>
                  <h4 class="text-center color-primary">

                    <% if(usuario.pais === 'Colombia') { %>
                    <% var valorFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(listaPlataformasNormales.valor); %>
                    <% } else { %>
                    <% var valorFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(listaPlataformasNormales.valor); %>
                    <% } %>
                    <%- valorFormateado %>

                  </h4>
                  <div class="text-center">
                    <button type="button" class="btn-informacion mt-2 mr-1" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalInformacion" onclick="modalInformacion(event, '<%= listaPlataformasNormales.plataforma.plataforma %>', '<%= listaPlataformasNormales.plataforma.logo %>', '<%= listaPlataformasNormales.plataforma.descripcion %>')"><i class="fas fa-info"></i></button>

                    <% const stock = cuentas.filter(function(element){ %>
                    <% return element.plataformaIdPlataforma == listaPlataformasNormales.plataforma.id_plataforma; %>
                    <% }); %>

                    <% if(stock.length === 0) { %>

                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinCuentas"><i class="ti ti-shopping-cart-full"></i> Comprar</button>

                    <% } else { %>

                    <% if (Number(usuario.saldo) >= Number(listaPlataformasNormales.valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraNormal" onclick="modalCompraNormal(event, '<%= listaPlataformasNormales.plataforma.id_plataforma %>', '<%= listaPlataformasNormales.plataforma.plataforma %>', '<%= listaPlataformasNormales.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>

                    <% } %>

                  </div>
                </div>
              </div>
            </div>
            <% }) %>
            <% } %>

          </div>


          <!-- =========================
                            Plataformas Bajo Pedido
                        ========================== -->

          <h2 class="main-content-title tx-24 mg-b-5 mt-4">Cuentas bajo pedido</h2>
          <p class="text-muted">Usted podra comprar las siguientes cuentas realizando la solicitud.</p>
          <div class="row row-sm">

            <% if(plataformasBajoPedido.length) { %>
            <% plataformasBajoPedido.forEach(listaplataformasBajoPedido => { %>

            <div class="col-sm-12 col-md-3">
              <div class="card custom-card our-team">
                <div class="card-body">
                  <div class="mb-3 circulo-etiqueta"></div>
                  <div class="picture avatar-lg online text-center">
                    <img alt="avatar" class="rounded-circle" src="<%- listaplataformasBajoPedido.plataforma.logo %>">
                  </div>
                  <div class="text-center mt-3">
                    <h5 class="pro-user-username text-dark mt-2 mb-0"><%- listaplataformasBajoPedido.plataforma.plataforma %></h5>
                    <p class="user-info-rating mt-2">
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                    </p>
                  </div>
                  <h4 class="text-center color-primary">

                    <% if(usuario.pais === 'Colombia') { %>
                    <% var valorFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(listaplataformasBajoPedido.valor); %>
                    <% } else { %>
                    <% var valorFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(listaplataformasBajoPedido.valor); %>
                    <% } %>
                    <%- valorFormateado %>
                  </h4>
                  <div class="text-center">
                    <button type="button" class="btn-informacion mt-2 mr-1" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalInformacion" onclick="modalInformacion(event, '<%= listaplataformasBajoPedido.plataforma.plataforma %>', '<%= listaplataformasBajoPedido.plataforma.logo %>', '<%= listaplataformasBajoPedido.plataforma.descripcion %>')"><i class="fas fa-info"></i></button>
                    <% if (Number(usuario.saldo) >= Number(listaplataformasBajoPedido.valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraPedido" onclick="modalCompraPedido(event, '<%= listaplataformasBajoPedido.plataforma.id_plataforma %>', '<%= listaplataformasBajoPedido.plataforma.plataforma %>', '<%= listaplataformasBajoPedido.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>
                  </div>
                </div>
              </div>
            </div>
            <% }) %>
            <% } %>

          </div>

          <!-- ===========================
                            Plataformas Renovaciones
                        ============================ -->

          <h2 class="main-content-title tx-24 mg-b-5 mt-4">Renovaciones</h2>
          <p class="text-muted">Usted podra renovar las cuentas compradas aqui, de cualquiera de las siguientes plataformas.</p>
          <div class="row row-sm">

            <% if(plataformasRenovaciones.length) { %>
            <% plataformasRenovaciones.forEach(listaplataformasRenovaciones => { %>

            <div class="col-sm-12 col-md-3">
              <div class="card custom-card our-team">
                <div class="card-body">
                  <div class="mb-3 circulo-etiqueta"></div>
                  <div class="picture avatar-lg online text-center">
                    <img alt="avatar" class="rounded-circle" src="<%- listaplataformasRenovaciones.plataforma.logo %>">
                  </div>
                  <div class="text-center mt-3">
                    <h5 class="pro-user-username text-dark mt-2 mb-0"><%- listaplataformasRenovaciones.plataforma.plataforma %></h5>
                    <p class="user-info-rating mt-2">
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                    </p>
                  </div>
                  <h4 class="text-center color-primary">
                    <% if(usuario.pais === 'Colombia') { %>
                    <% var valorFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(listaplataformasRenovaciones.valor); %>
                    <% } else { %>
                    <% var valorFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(listaplataformasRenovaciones.valor); %>
                    <% } %>
                    <%- valorFormateado %>
                  </h4>
                  <div class="text-center">
                    <button type="button" class="btn-informacion mt-2 mr-1" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalInformacion" onclick="modalInformacion(event, '<%= listaplataformasRenovaciones.plataforma.plataforma %>', '<%= listaplataformasRenovaciones.plataforma.logo %>', '<%= listaplataformasRenovaciones.plataforma.descripcion %>')"><i class="fas fa-info"></i></button>

                    <% if (Number(usuario.saldo) >= Number(listaplataformasRenovaciones.valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraRenovacion" onclick="modalCompraRenovacion(event, '<%= listaplataformasRenovaciones.plataforma.id_plataforma %>', '<%= listaplataformasRenovaciones.plataforma.plataforma %>', '<%= listaplataformasRenovaciones.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>

                  </div>
                </div>
              </div>
            </div>
            <% }) %>
            <% } %>

          </div>


          <!-- ============================
                            Plataformas Personalizadas
                        ============================= -->

          <h2 class="main-content-title tx-24 mg-b-5 mt-4">Personalizadas</h2>
          <p class="text-muted">Usted podra comprar las siguientes cuentas con correos personalizados.</p>
          <div class="row row-sm">

            <% if(plataformasPersonalizadas.length) { %>
            <% plataformasPersonalizadas.forEach(listaPlataformasPersonalizadas => { %>

            <div class="col-sm-12 col-md-3">
              <div class="card custom-card our-team">
                <div class="card-body">
                  <div class="mb-3 circulo-etiqueta"></div>
                  <div class="picture avatar-lg online text-center">
                    <img alt="avatar" class="rounded-circle" src="<%- listaPlataformasPersonalizadas.plataforma.logo %>">
                  </div>
                  <div class="text-center mt-3">
                    <h5 class="pro-user-username text-dark mt-2 mb-0"><%- listaPlataformasPersonalizadas.plataforma.plataforma %></h5>
                    <p class="user-info-rating mt-2">
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                    </p>
                  </div>
                  <h4 class="text-center color-primary">
                    <% if(usuario.pais === 'Colombia') { %>
                    <% var valorFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(listaPlataformasPersonalizadas.valor); %>
                    <% } else { %>
                    <% var valorFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(listaPlataformasPersonalizadas.valor); %>
                    <% } %>
                    <%- valorFormateado %>
                  </h4>
                  <div class="text-center">
                    <button type="button" class="btn-informacion mt-2 mr-1" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalInformacion" onclick="modalInformacion(event, '<%= listaPlataformasPersonalizadas.plataforma.plataforma %>', '<%= listaPlataformasPersonalizadas.plataforma.logo %>', '<%= listaPlataformasPersonalizadas.plataforma.descripcion %>')"><i class="fas fa-info"></i></button>

                    <% if (Number(usuario.saldo) >= Number(listaPlataformasPersonalizadas.valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraPersonalizada" onclick="modalCompraPersonalizada(event, '<%= listaPlataformasPersonalizadas.plataforma.id_plataforma %>', '<%= listaPlataformasPersonalizadas.plataforma.plataforma %>', '<%= listaPlataformasPersonalizadas.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>

                  </div>
                </div>
              </div>
            </div>
            <% }) %>
            <% } %>

          </div>


          <!-- ======================
                            Plataformas Juegos
                        ======================= -->

          <h2 class="main-content-title tx-24 mg-b-5 mt-4">Juegos</h2>
          <p class="text-muted">Usted podra comprar los siguientes pines, diamantes, monedas coins, etc.</p>
          <div class="row row-sm">

            <% if(plataformasJuegos.length) { %>
            <% plataformasJuegos.forEach(listaPlataformasJuegos => { %>

            <div class="col-sm-12 col-md-3">
              <div class="card custom-card our-team">
                <div class="card-body">
                  <div class="mb-3 circulo-etiqueta"></div>
                  <div class="picture avatar-lg online text-center">
                    <img alt="avatar" class="rounded-circle" src="<%- listaPlataformasJuegos.plataforma.logo %>">
                  </div>
                  <div class="text-center mt-3">
                    <h5 class="pro-user-username text-dark mt-2 mb-0"><%- listaPlataformasJuegos.plataforma.plataforma %></h5>
                    <p class="user-info-rating mt-2">
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                      <a href="#"><i class="fa fa-star text-warning"> </i></a>
                    </p>
                  </div>
                  <h4 class="text-center color-primary">
                    <% const array = asignaciones.filter(function(element){ %>
                    <% return element.plataformaIdPlataforma == listaPlataformasJuegos.plataforma.id_plataforma; %>
                    <% }); %>

                    <% const valor = array[0].valor; %>

                    <% if(usuario.pais === 'Colombia') { %>
                    <% var valorFormateado = new Intl.NumberFormat("es-CO", {style: "currency", currency: "COP", maximumSignificantDigits: 7}).format(valor); %>
                    <% } else { %>
                    <% var valorFormateado = new Intl.NumberFormat("en-US", {style: "currency", currency: "USD"}).format(valor); %>
                    <% } %>
                    <%- valorFormateado %>
                  </h4>
                  <div class="text-center">
                    <button type="button" class="btn-informacion mt-2 mr-1" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalInformacion" onclick="modalInformacion(event, '<%= listaPlataformasJuegos.plataforma.plataforma %>', '<%= listaPlataformasJuegos.plataforma.logo %>', '<%= listaPlataformasJuegos.plataforma.descripcion %>')"><i class="fas fa-info"></i></button>

                    <% const nomPlataforma = listaPlataformasJuegos.plataforma.plataforma.toLowerCase(); %>

                    <% if(nomPlataforma.includes('free fire')) { %>

                    <% if (Number(usuario.saldo) >= Number(valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraFreefire" onclick="modalCompraFreefire(event, '<%= listaPlataformasJuegos.plataforma.id_plataforma %>', '<%= listaPlataformasJuegos.plataforma.plataforma %>', '<%= listaPlataformasJuegos.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>

                    <% } else if(nomPlataforma.includes('call of duty')) { %>

                    <% if (Number(usuario.saldo) >= Number(listaPlataformasJuegos.valor)) { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalCompraCallofduty" onclick="modalCompraCallofduty(event, '<%= listaPlataformasJuegos.plataforma.id_plataforma %>', '<%= listaPlataformasJuegos.plataforma.plataforma %>', '<%= listaPlataformasJuegos.plataforma.logo %>')"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } else { %>
                    <button type="button" class="btn-comprar mt-2" data-effect="effect-flip-horizontal" data-toggle="modal" href="#modalSinSaldo"><i class="ti ti-shopping-cart-full"></i> Comprar</button>
                    <% } %>

                    <% } %>

                  </div>
                </div>
              </div>
            </div>
            <% }) %>
            <% } %>

          </div>


          <!-- =============================
							Fin Componentes Pagina
						============================== -->

        </div>
      </div>
    </div>
    <!-- End Main Content-->

    <!-- Foot -->
    <%- include('layautsDashboard/foot') %>

    <!-- Notificiaciones -->
    

  </div>
  <!-- End Page -->

  <!-- Modales -->

  <!-- Modal info -->
  <div class="modal" id="modalInformacion">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content modal-content-demo" style="border-radius: 10px;">
        <div class="modal-header">
          <h6 class="modal-title">Descripción de la plataforma</h6><button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">

          <h3 class="text-center text-uppercase" id="nombrePlataforma"></h3>

          <center>
            <div class="picture img-descripcion online text-center my-4">
              <img alt="avatar" class="rounded-circle" id="logoInfo" src="">
            </div>
          </center>

          <p class="text-left mb-3" id="descripcionPltaforma"></p>

        </div>
      </div>
    </div>
  </div>

  <!-- Modal sin saldo -->
  <div class="modal" id="modalSinSaldo">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content modal-content-demo" style="border-radius: 10px;">
        <div class="modal-header">
          <h6 class="modal-title">Saldo insuficiente</h6><button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">

          <h3 class="text-center text-uppercase" id="nombrePlataforma"></h3>

          <center>
            <div class="picture img-descripcion online text-center my-4">
              <img alt="avatar" src="/assetsDashboard/img/empty.svg">
            </div>
          </center>

          <p class="text-center mb-3" style="color: #4a526f;">¡Lo sentimos!, no tienes saldo suficiente para comprar esta cuenta, te invitamos a recargar tu cuenta.</p>

        </div>
        <div class="modal-footer">
          <a class="btn btn-primary" href="/dashboard/reportarConsignacion">Cargar mi cuenta</a>
          <button class="btn ripple btn-secondary" data-dismiss="modal" type="button">Cerrar</button>
        </div>
      </div>
    </div>
  </div>

  <!-- Modal sin Cuentas -->
  <div class="modal" id="modalSinCuentas">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content modal-content-demo" style="border-radius: 10px;">
        <div class="modal-header">
          <h6 class="modal-title">...Ups! Lo sentimos</h6><button aria-label="Close" class="close" data-dismiss="modal" type="button"><span aria-hidden="true">&times;</span></button>
        </div>
        <div class="modal-body">

          <h3 class="text-center text-uppercase" id="nombrePlataforma"></h3>

          <center>
            <div class="picture img-descripcion online text-center my-4">
              <img alt="avatar" src="/assetsDashboard/img/inbox.svg">
            </div>
          </center>

          <p class="text-center mb-3" style="color: #4a526f;">¡Error NHC0073!, no puedes solicitar cuentas de esta plataforma en este momento, estamos trabajando en ello, por favor vuelve a intentarlo en un rato.</p>


    function comprarCuentaNormal(e) {

      const clienteCompraNormal = document.getElementById('clienteCompraNormal').value;
      const telefonoCliente = document.getElementById('mobile-number').value;
      const autorizacionNormal = document.getElementById('autorizacionNormal').checked;
      const id = document.getElementById("idPlataformaCompraNormal").value;

      if (!(clienteCompraNormal == '' || telefonoCliente == '')) {

        if (autorizacionNormal) {

          $('#global-loader').addClass("d-block");
          $('#global-loader').removeClass("d-none");
          fetch('/plataformas/compraCuentaNormal', {
              method: 'POST',
              headers: {
                'Content-type': 'application/json'
              },
              body: JSON.stringify({
                id: id,
                cliente: clienteCompraNormal,
                telefono: telefonoCliente
              })
            })
            .then(res => res.text())
            .then(data => {
              $('#global-loader').removeClass("d-block");
              $('#global-loader').addClass("d-none");
              const respuesta = JSON.parse(data);
              if (respuesta.resp === 'success') {

                const seccionUno = $("#seccionCompraNormal");
                const footerUno = $("#footerCompraNormal");
                seccionUno.fadeOut(300).delay(800);
                footerUno.fadeOut(300).delay(800);

                const usuarioCuentaNormal = document.getElementById('usuarioCuentaNormal');
                let respuestaUser = respuesta.user;
                let pre_respuestaUser = respuestaUser;
                respuestaUser = respuestaUser.replaceAll('+', '%2B');
                respuestaUser = respuestaUser.replaceAll('&', '%26');
                usuarioCuentaNormal.innerHTML = pre_respuestaUser;

                const passwordCuentaNormal = document.getElementById('passwordCuentaNormal');
                let respuestaPassword = respuesta.password;
                let pre_respuestaPassword = respuestaPassword;
                respuestaPassword = respuestaPassword.replaceAll('+', '%2B');
                respuestaPassword = respuestaPassword.replaceAll('&', '%26');
                passwordCuentaNormal.innerHTML = pre_respuestaPassword;

                const pantallaCuentaNormal = document.getElementById('pantallaCuentaNormal');
                pantallaCuentaNormal.innerHTML = respuesta.pantalla;

                const pinCuentaNormal = document.getElementById('pinCuentaNormal');
                pinCuentaNormal.innerHTML = respuesta.pin;

                const imgCompraNormal = document.getElementById('imgCompraNormal');
                imgCompraNormal.src = respuesta.logo;

                const btnWhatsappNormal = document.getElementById('btnWhatsappNormal');

                btnWhatsappNormal.href = 'https://api.whatsapp.com/send?phone=' + respuesta.telefono + '&text=Hola!%20%F0%9F%98%80%20%20*' + respuesta.cliente + '*%2C%20estos%20son%20los%20datos%20de%20tu%20cuenta.%20%0A%0A%F0%9F%93%B2%20%20*PLATAFORMA%3A*%20' + respuesta.plataforma + '%20%0A%F0%9F%99%8D%E2%80%8D%E2%99%82%EF%B8%8F%20%20*USUARIO%3A*%20' + respuestaUser + '%20%0A%F0%9F%94%91%20%20*CONTRASE%C3%91A%3A*%20' + respuestaPassword + '%20%0A%F0%9F%93%8C%20*Pantalla%3A*%20' + respuesta.pantalla + '%20%0A%F0%9F%93%8C%20*Pin%3A*%20' + respuesta.pin;

                const seccionDos = $("#seccionCompraNormalRealizado");
                const footerDos = $("#footerCompraNormalRealizado");
                seccionDos.fadeIn(300).delay(800);
                footerDos.fadeIn(300).delay(800);

              } else {

                $('#alertaComprarNormal').fadeIn();
                const error = '<button aria-label="Close" class="close" data-dismiss="alert" type="button"> <span aria-hidden="true" class="text-danger">×</span> </button> <strong>Oh no!</strong> ' + respuesta.descripcion + '.';
                $('#alertaComprarNormal').html(error);
                $('#alertaComprarNormal').delay(2000).fadeOut();
              }
            }).catch((err) => {
              console.log(err)
              $('#global-loader').removeClass("d-block");
              $('#global-loader').addClass("d-none");
              swal({
                title: 'Request Error',
                text: '500 - Plataformas',
                type: 'error'
              });
            })

        } else {

          $('#alertaComprarNormal').fadeIn();
          const error = '<button aria-label="Close" class="close" data-dismiss="alert" type="button"> <span aria-hidden="true" class="text-danger">×</span> </button> <strong>Oh no!</strong> Por favor autorize el envio de datos.';
          $('#alertaComprarNormal').html(error);
          $('#alertaComprarNormal').delay(2000).fadeOut();
        }

      } else {

        $('#alertaComprarNormal').fadeIn();
        const error = '<button aria-label="Close" class="close" data-dismiss="alert" type="button"> <span aria-hidden="true" class="text-danger">×</span> </button> <strong>Oh no!</strong> Por favor llene todos los campos.';
        $('#alertaComprarNormal').html(error);
        $('#alertaComprarNormal').delay(2000).fadeOut();

      }

    }

